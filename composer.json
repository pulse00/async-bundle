{
  "name": "dubture/async-bundle",
  "description": "Provides an easy to use abstraction for async backend workers in Symfony2",
  "authors": [
    {
      "name": "Robert Gruendler",
      "email": "robert@dubture.com"
    }
  ],
  "type": "symfony-bundle",
  "license": "MIT",
  "keywords": [
    "symfony",
    "async",
    "rabbitmq",
    "resque",
    "background"
  ],
  "require": {
    "jms/di-extra-bundle": "~1.5"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.5",
    "symfony/finder": "~2.2",
    "symfony/browser-kit": "~2.2",
    "symfony/monolog-bundle": "~2.2",
    "bcc/resque-bundle": "~1.1",
    "oldsound/rabbitmq-bundle": "~1.6",
    "sonata-project/notification-bundle": "~2.3",
    "sonata-project/admin-bundle": "~2.3",
    "doctrine/orm": "~2.2,>=2.2.3,<2.5",
    "doctrine/dbal": "<2.5",
    "doctrine/doctrine-bundle": "~1.2",
    "symfony/swiftmailer-bundle": "~2.3",
    "sensio/framework-extra-bundle": "~3.0,>=3.0.2"
  },
  "autoload": {
    "psr-4": {"Dubture\\AsyncBundle\\": ""}
  },
  "suggest": {
    "oldsound/rabbitmq-bundle": "Backend implementation using rabbitmq",
    "bcc/resque-bundle": "Backend implementation using resque",
    "sonata-project/notification-bundle": "The sonata-notification bundle provides multiple backends to choose from"
  }
}
